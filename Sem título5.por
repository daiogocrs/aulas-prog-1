programa {
  funcao inicio() 
  {
    inteiro contador

    para (contador = 0; contador <= 10; contador += 2) {
      escreva("Valor do contador: ", contador, "\n")
    }
    para (contador = 10; contador >= 0; contador -=2) {
      escreva("Valor do contador: ", contador, "\n")
    }
    escreva("Valor do contador: ", contador, "\n")
  }
}
