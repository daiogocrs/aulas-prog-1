programa {
  funcao inicio() 
  {
    inteiro numero
    
    escreva("Digite um número inteiro ")
    leia(numero)

    escolha (numero) {
      caso 1: escreva("Número um\n")
        pare
      caso 2: escreva("Número dois\n")
        pare
      caso 3: escreva("Número três\n")
        pare
      caso contrario: escreva("Número inválido!")
    }
    
  }
}
